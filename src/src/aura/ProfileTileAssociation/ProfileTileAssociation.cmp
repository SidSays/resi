<aura:component implements="flexipage:availableForAllPageTypes" access="global" >
    
    <div class="slds-picklist slds-dropdown-trigger slds-dropdown-trigger--click slds-is-open slds-p-bottom_x-large">
  		<div class="slds-form-element">
    		<label class="slds-form-element__label" for="text-input-01">Profiles</label>
            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon--right slds-picklist__input">
                <input type="search" id="text-input-01" class="slds-lookup__search-input slds-input" placeholder="Retail Sales" aria-owns="option-list-01" role="combobox" aria-activedescendant="" aria-expanded="true" readonly="" />
                <lightning:buttonicon iconName="utility:down" size="x-small" class="slds-input__icon slds-text-color--default" alternativeText="Expand category options"/>                        
            </div>
  		</div>  		
	</div>
    
	<div class="slds-dueling-list slds-p-bottom_x-large">
        <div class="slds-assistive-text" id="drag-live-region" aria-live="assertive"></div>
        <div class="slds-assistive-text" id="option-drag-label">Press space bar when on an item, to move it within the list. CMD plus left and right arrow keys, to move items between lists.</div>
        <div class="slds-dueling-list__column">
            <span class="slds-form-element__label" id="label-45">Available Tiles</span>
            <div class="slds-dueling-list__options" role="application">
                <ul aria-describedby="option-drag-label" aria-labelledby="label-45" aria-multiselectable="true" class="slds-listbox slds-listbox_vertical" role="listbox" tabindex="0">
                    <li role="presentation" class="slds-listbox__item">
                        <span class="slds-listbox__option slds-listbox__option_plain slds-media slds-is-selected" aria-selected="true" draggable="true" role="option" tabindex="0">
                            <span class="slds-truncate" title="Option 1">Dashboard</span>
                        </span>
                    </li>
                    <li role="presentation" class="slds-listbox__item">
                        <span class="slds-listbox__option slds-listbox__option_plain slds-media" aria-selected="false" draggable="true" role="option" tabindex="-1">
                            <span class="slds-truncate" title="Option 2">MyInfoQuest</span>
                        </span>
                    </li>
                    <li role="presentation" class="slds-listbox__item">
                        <span class="slds-listbox__option slds-listbox__option_plain slds-media" aria-selected="false" draggable="true" role="option" tabindex="-1">
                            <span class="slds-truncate" title="Option 3">OrderLab</span>
                        </span>
                    </li>
                </ul>
            </div>
        </div>
        
        <div class="slds-dueling-list__column">
            <lightning:icon iconName="utility:right" size="x-small" class="slds-button slds-button_icon-container" alternativeText="Move Selection to Second Category"/>                        
            <lightning:icon iconName="utility:left" size="x-small" class="slds-button slds-button_icon-container" alternativeText="Move Selection to First Category"/>                        
        </div>
        
        <div class="slds-dueling-list__column">
            <span class="slds-form-element__label" id="label-46">Selected Tiles</span>
            <div class="slds-dueling-list__options" role="application">
                <ul aria-describedby="option-drag-label" aria-labelledby="label-46" aria-multiselectable="true" class="slds-listbox slds-listbox_vertical" role="listbox" tabindex="0">
                    <li role="presentation" class="slds-listbox__item">
                        <span class="slds-listbox__option slds-listbox__option_plain slds-media" aria-selected="false" draggable="true" role="option" tabindex="0">
                            <span class="slds-truncate" title="Option 4">ComcastNow</span>
                        </span>
                    </li>
                </ul>
            </div>
        </div>
        
        <div class="slds-dueling-list__column">
            <lightning:icon iconName="utility:up" size="x-small" class="slds-button slds-button_icon-container" alternativeText="Move Selection Up"/>                        
            <lightning:icon iconName="utility:down" size="x-small" class="slds-button slds-button_icon-container" alternativeText="Move Selection Down"/>
        </div>
    </div>
    
    <div>
    	<button class="slds-button slds-button_neutral">Cancel</button>
        <button class="slds-button slds-button_neutral">Save</button>
    </div>
</aura:component>