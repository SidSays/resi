<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <fieldUpdates>
        <fullName>AddressCodeUpdate1</fullName>
        <field>Address_Code__c</field>
        <formula>House_Misc_Code__c+&apos;_&apos;+ Sys__c  +  Prin__c +&apos;_&apos;+ TEXT(Property_Region__c)</formula>
