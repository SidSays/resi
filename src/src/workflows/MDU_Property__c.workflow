IF((LEN(Billing_Corp__c))=3 , Complex_Code__c+&apos;_&apos;+&apos;00&apos;+Billing_Corp__c+&apos;_&apos;+TEXT(Property_Region__c), 
IF((LEN(Billing_Corp__c))=2 , Complex_Code__c+&apos;_&apos;+&apos;000&apos;+Billing_Corp__c+&apos;_&apos;+TEXT(Property_Region__c), 
IF((LEN(Billing_Corp__c))=1 , Complex_Code__c+&apos;_&apos;+&apos;0000&apos;+Billing_Corp__c+&apos;_&apos;+TEXT(Property_Region__c), 
Complex_Code__c+&apos;_&apos;+ Billing_Corp__c+&apos;_&apos;+TEXT(Property_Region__c)))))</formula>
        <name>AddressCodeUpdate2</name>
        <notifyAssignee>false</notifyAssignee>
